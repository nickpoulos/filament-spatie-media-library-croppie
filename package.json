{
    "name": "filament-spatie-media-library-croppie",
    "scripts": {
        "build:css": "node-sass resources/css/filament-spatie-media-library-croppie.css resources/dist/css/filament-spatie-media-library-croppie.css --output-style compressed --importer node_modules/node-sass-import",
        "build:js": "esbuild resources/js/filament-spatie-media-library-croppie.js --bundle --minify --outfile=resources/dist/js/filament-spatie-media-library-croppie.js"
